{
  "name": "react-thumbnail-gallery",
  "version": "2.0.2",
  "description": "A responsive image gallery featuring (1) a pane showing the full image, and (2) a pane with image thumbnails, titles, and descriptions.",
  "main": "src/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwatkins0/react-thumbnail-gallery.git"
  },
  "scripts": {
    "start": "parcel example/src/index.html -d example -o --public-url /react-thumbnail-gallery/example/",
    "build": "parcel build example/src/index.html -d example --public-url /react-thumbnail-gallery/example/",
    "lint": "eslint src",
    "prepare": "npm run build",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "keywords": [
    "wordpress",
    "react"
  ],
  "author": "John Watkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnwatkins0/react-thumbnail-gallery/issues"
  },
  "homepage": "https://github.com/johnwatkins0/react-thumbnail-gallery#readme",
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "parcel-bundler": "^1.3.1",
    "react-test-renderer": "^16.2.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.2.3"
  }
}
